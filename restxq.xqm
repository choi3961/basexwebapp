(:~
 : This function returns the result of a form request.
 
 :)
 (:function sentences:)
declare
  %rest:path("/bookedit/sentences")
  %rest:POST
  %rest:form-param("num","{$num}", "(no message)")
  %rest:form-param("tag","{$tag}", "(no message)")
  %output:method("xhtml")
  %output:omit-xml-declaration("no")
  %output:doctype-public("-//W3C//DTD XHTML 1.0 Transitional//EN")
  %output:doctype-system("http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd")
  function page:sentences($num as xs:int, $tag as xs:string)
    {
        for $data in doc("sentences.xml")/sentences/sentence[tags/tag=$tag]
        order by  xs:integer($data/tags/length)
        return 
        <p>{$data/text()}</p>
    };
 
 
